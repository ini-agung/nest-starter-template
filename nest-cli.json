      "compilerOptions": {
        "tsConfigPath": "libs/seeder/tsconfig.lib.json"
      }
    },
    "helper": {
      "type": "library",
      "root": "libs/helper",
      "entryFile": "index",
      "sourceRoot": "libs/helper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helper/tsconfig.lib.json"
      }
