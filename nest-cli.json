{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "response": {
      "type": "library",
      "root": "libs/response",
      "entryFile": "index",
      "sourceRoot": "libs/response/src",
      "compilerOptions": {
        "tsConfigPath": "libs/response/tsconfig.lib.json"
      }
    },
    "connections": {
      "type": "library",
      "root": "libs/connections",
      "entryFile": "index",
      "sourceRoot": "libs/connections/src",
      "compilerOptions": {
        "tsConfigPath": "libs/connections/tsconfig.lib.json"
      }
    },
    "auth": {
