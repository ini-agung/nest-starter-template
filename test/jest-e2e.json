{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/response/(.*)": "<rootDir>/../libs/response/src/$1",
    "@app/response": "<rootDir>/../libs/response/src",
    "@app/connections/(.*)": "<rootDir>/../libs/connections/src/$1",
    "@app/connections": "<rootDir>/../libs/connections/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/jwt/(.*)": "<rootDir>/../libs/jwt/src/$1",
    "@app/jwt": "<rootDir>/../libs/jwt/src",
    "@app/jwt-auth/(.*)": "<rootDir>/../libs/jwt-auth/src/$1",
    "@app/jwt-auth": "<rootDir>/../libs/jwt-auth/src",
    "@app/jwt-libs/(.*)": "<rootDir>/../libs/jwt-libs/src/$1",
    "@app/jwt-libs": "<rootDir>/../libs/jwt-libs/src"
  }
}